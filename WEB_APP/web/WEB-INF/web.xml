<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <servlet>
        <servlet-name>MyServlet</servlet-name>
        <servlet-class>main.java.com.ahmet.MyServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>MyServlet</servlet-name>
        <url-pattern>/AhmetTekden</url-pattern>
    </servlet-mapping>

    <servlet>
    <servlet-name>EzgisServlet</servlet-name>
    <servlet-class>main.java.com.ezgi.EzgisServlet</servlet-class>
</servlet>

    <servlet-mapping>
        <servlet-name>EzgisServlet</servlet-name>
        <url-pattern>/EzgiTekdemir</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Irem</servlet-name>
        <servlet-class>main.java.com.irem.Irem</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Irem</servlet-name>
        <url-pattern>/IremZog</url-pattern>
    </servlet-mapping>

</web-app>
